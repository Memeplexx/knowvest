{
  "name": "knowledge-harvest",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "dev:ts": "rm public/sw.js & npm run dev & npm run ts:watch",
    "ts": "tsc --noEmit --incremental",
    "ts:watch": "yarn ts --watch",
    "migrate:local": "npx prisma migrate dev --skip-seed",
    "postinstall": "prisma generate",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "prisma:generate": "prisma generate",
    "prisma:create-db": "npx prisma migrate dev --name init",
    "test": "vitest"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.9.0",
    "@codemirror/commands": "^6.2.5",
    "@codemirror/lang-javascript": "^6.2.1",
    "@codemirror/lang-markdown": "^6.2.0",
    "@codemirror/language": "^6.9.0",
    "@codemirror/language-data": "^6.3.1",
    "@codemirror/lint": "^6.4.1",
    "@codemirror/search": "^6.5.2",
    "@codemirror/state": "^6.2.1",
    "@codemirror/view": "^6.17.1",
    "@floating-ui/react": "^0.25.3",
    "@lezer/highlight": "^1.1.6",
    "@lezer/markdown": "^1.1.0",
    "@vercel/analytics": "^1.2.0",
    "@vercel/speed-insights": "^1.0.10",
    "autoprefixer": "10.4.14",
    "codemirror": "^6.0.1",
    "date-fns": "^2.30.0",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.4",
    "next": "14.1.0",
    "next-auth": "^4.24.6",
    "next-pwa": "^5.6.0",
    "olik": "1.0.108",
    "olik-react": "1.0.20",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-content-loader": "^6.2.1",
    "react-dom": "18.2.0",
    "react-icons": "^4.10.1",
    "react-spring": "^9.7.2",
    "setimmediate": "^1.0.5",
    "sharp": "^0.32.5",
    "styled-components": "6.0.7"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.4.19",
    "@prisma/client": "^5.9.1",
    "@testing-library/react": "^14.2.1",
    "@types/codemirror": "^5.60.9",
    "@types/node": "^20.5.7",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitejs/plugin-react": "^4.2.1",
    "jsdom": "^24.0.0",
    "prisma": "^5.9.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.3.0"
  }
}
