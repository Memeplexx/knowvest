{
  "name": "knowledge-harvest",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "dev:ts": "rm public/sw.js & npm run dev & npm run ts:watch",
    "ts": "tsc --noEmit --incremental",
    "ts:watch": "yarn ts --watch",
    "migrate:local": "npx prisma migrate dev --skip-seed",
    "postinstall": "prisma generate",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "prisma:generate": "prisma generate",
    "prisma:create-db": "npx prisma migrate dev --name init"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.6.0",
    "@codemirror/commands": "^6.2.3",
    "@codemirror/lang-javascript": "^6.1.7",
    "@codemirror/lang-markdown": "^6.1.1",
    "@codemirror/language": "^6.6.0",
    "@codemirror/language-data": "^6.3.0",
    "@codemirror/lint": "^6.2.1",
    "@codemirror/search": "^6.4.0",
    "@codemirror/state": "^6.2.0",
    "@codemirror/view": "^6.10.0",
    "@floating-ui/react": "^0.25.1",
    "@lezer/highlight": "^1.1.4",
    "@lezer/markdown": "^1.1.0",
    "@trpc/client": "^10.24.0",
    "@trpc/next": "^10.24.0",
    "@trpc/server": "^10.24.0",
    "@vercel/analytics": "^1.0.2",
    "@vercel/speed-insights": "^1.0.2",
    "autoprefixer": "10.4.14",
    "codemirror": "^6.0.1",
    "date-fns": "^2.30.0",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.4",
    "next": "13.3.4",
    "next-auth": "^4.22.4",
    "next-pwa": "^5.6.0",
    "next-superjson": "^0.0.4",
    "olik": "1.0.33",
    "olik-react": "1.0.10",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-content-loader": "^6.2.1",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0",
    "react-spring": "^9.7.1",
    "setimmediate": "^1.0.5",
    "sharp": "^0.32.5",
    "styled-components": "6.0.7",
    "superjson": "^1.12.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.4.19",
    "@prisma/client": "^5.2.0",
    "@types/codemirror": "^5.60.7",
    "@types/node": "^20.2.5",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "@types/styled-components": "^5.1.26",
    "@types/styled-system": "^5.1.16",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "prisma": "^5.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
